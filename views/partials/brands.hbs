<div class="center" style="margin: 10px 0">
    <h3 class="left-align">Популярные бренды</h3>
    <div class="row center">
        {{#each brands}}
            <a class="col s12 m6 l3" href="{{link}}">
                <div class="card-panel hoverable" style="padding: 20px 10px; border-radius: 10px; height: 100px; background: {{backColor}};">
                    <img src="{{photoURL}}" alt="" style="max-width: 100%; max-height: 100%">
                </div>
            </a>
        {{/each}}
    </div>
</div>